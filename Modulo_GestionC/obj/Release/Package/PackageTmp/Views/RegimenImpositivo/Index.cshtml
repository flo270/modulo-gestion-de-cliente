@model IEnumerable<Modulo_GestionC.Models.RegimenImpositivo>

@{
    ViewBag.Title = "RegimenImpositivo";
}

<h2>Regimen Impositivo</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Crear")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.NombreRegimenImpositivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Categoria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRgistro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.NombreRegimenImpositivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Categoria)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaRgistro)
        </td>
        <td>
          
            @Html.ActionLink("Detallar", "Details", new { id = item.IdRI }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.IdRI })
        </td>
    </tr>
}

</table>
